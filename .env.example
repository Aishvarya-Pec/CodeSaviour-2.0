# Copy to .env for local development only
# Never commit real API keys. Use Cloudflare Wrangler secrets in production.

# Fireworks AI (optional, for local FastAPI server)
FIREWORKS_API_KEY=
FIREWORKS_BASE_URL=https://api.fireworks.ai/inference/v1
FIREWORKS_MODEL=accounts/fireworks/models/qwen2p5-coder-32b-instruct
FIREWORKS_MODELS=accounts/fireworks/models/qwen2p5-coder-32b-instruct,accounts/fireworks/models/llama-v3p1-70b-instruct,accounts/fireworks/models/llama-v3p1-8b-instruct

# OpenRouter (used by Cloudflare Worker and optionally local server)
OPENROUTER_API_KEY=
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
# Example default; override with a paid model if desired
OPENROUTER_MODEL=qwen/qwen-2.5-coder-32b-instruct:free

# Site configuration
SITE_URL=http://127.0.0.1:8000/
SITE_TITLE=CodeSaviour

# CORS (comma-separated list). Include your deployed domain if hosting elsewhere.
CORS_ALLOW_ORIGINS=http://127.0.0.1:8000,http://localhost:8000

# Analyzer path overrides (optional)
FLAKE8_PATH=
ESLINT_PATH=
CPP_CHECK_PATH=
CLANG_TIDY_PATH=
PMD_PATH=